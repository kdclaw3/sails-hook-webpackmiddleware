{
	"env": {
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"allowImportExportEverywhere": false,
		"codeFrame": false,
		"ecmaFeatures": {
			"jsx": true,
			"experimentalObjectRestSpread": true
		}
	},
	"extends": [
		"eslint:recommended"
	],
	"rules": {
		"strict": 0,
		"comma-dangle": 1,
		"indent": ["warn","tab"],
		"no-tabs": 0,
		"no-console": "warn",
		"no-unused-vars": "warn",
		"eqeqeq": "warn"
	}
}